{
    "name": "zemlia-zemlia/yii2-images",
    "description": "yii2 images module for storing images fix already header",
    "keywords": ["yii2", "images", "store", "images store", "yii2-images", "Costa Rico"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",

    "authors": [
        {
            "name": "zemlia-zemlia",
            "email": "zemlia-zemlia@mail.ru",
            "homepage": "https://github.com/zemlia-zemlia/yii2-images"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "abeautifulsite/simpleimage": "2.7"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "mikey179/vfsstream": "1.3.*@dev"
    },
    "suggest": {
        "ext-imagick": ""
    },
    "autoload": {
        "psr-4": {
            "zemlia-zemlia\\yii2images\\": ""
        }
    }
}
